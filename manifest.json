{
    "manifest_version": 2,
    "name": "HDrezka bookmarks",
    "version": "1.0.0",
    "background": {
        "persistent": true,
        "scripts": ["background.js", "jquery.js"]
    },
    "permissions": [
        "http://hdrezka.me/*",
        "activeTab",
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
